          create_host_path: true
      - type: bind
        source: C:\Users\Thien.huynh2\AppData\Roaming/ASP.NET/Https
        target: C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https
        read_only: true
        bind:
          create_host_path: true
